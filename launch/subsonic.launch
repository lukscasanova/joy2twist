<launch>

 <!-- Joy2Vero Node-->
  <node pkg="joy2twist" type="joy2twist_node" name="joy2twist">
  </node>

  <!-- Axes -->
  <param name="vel_topic" value="cmd_vel" />
  <param name="axis_linear" value="3" type="int"/>
  <param name="axis_angular" value="0" type="int"/>
  <param name="dforward" value="" />
  <param name="dbackward" value="" />
  <param name="dright" value="" />
  <param name="dleft" value="" />
  <param name="zerovel" value="" />
  <param name="scale_linear" value="0.3" type="double"/>
  <param name="scale_angular" value="0.4" type="double"/>


</launch>

